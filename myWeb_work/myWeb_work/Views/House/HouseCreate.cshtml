@model myWeb_work.Models.House


@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>HouseCreate</title>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

</head>
<body>
    <div class="container">
        <h2>House Create</h2>
        <form action="SubmitCreate" method="post">
            <div class="form-group">
                <label for="HouseName">House Name:</label>
                <input type="text" class="form-control" id="HouseName" placeholder="Enter House Name:" name="HouseName" value="@Model.HouseName" />@Html.ValidationMessageFor(x => x.HouseName)
            </div>
            <div class="form-group">
                <label for="HouseAddress">House Address:</label>
                <input type="text" class="form-control" id="HouseAddress" placeholder="Enter House Address:" name="HouseAddress" value="@Model.HouseAddress">@Html.ValidationMessageFor(x => x.HouseAddress)
            </div>
            <div class="form-group">
                <label for="HouseDetails">House Details::</label>
                <input type="text" class="form-control" id="HouseDetails" placeholder="Enter House Details:" name="HouseDetails" value="@Model.HouseDetails" />@Html.ValidationMessageFor(x => x.HouseDetails)
            </div>
            <div class="form-group">
                <label for="HousePrice">House Price::</label>
                <input type="text" class="form-control" id="HousePrice" placeholder="Enter House Price" name="HousePrice" value="@Model.HousePrice" />@Html.ValidationMessageFor(x => x.HousePrice)
            </div>
            <div>@ViewBag.Error<p></p></div>
            <button type="submit" class="btn btn-default">Ok</button><button type="button" class="btn btn-default" onclick="history.go(-1);">Cancel</button>
        </form>
    </div>
</body>
</html>
