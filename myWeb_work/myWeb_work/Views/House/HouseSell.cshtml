@model List<myWeb_work.Models.House>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Houses Buy</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery-3.2.1.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>

    <style>
          .body{background:#2d2d30}
          .bg-1 {
            background: #2d2d30;
            }
           .bg-1 h3 {color: #fff;}
           .bg-1 p {font-style: italic;}
           .bu {margin:5px;}
           .text-center{align-items}
    </style>
</head>
<body>
    @Html.Partial("~/Views/Shared/BarActionUser.cshtml", (object)ViewBag.user)
    <div class="jumbotron text-center">
        <h1>Buy Your Dream House</h1>
        <p>Here you can buy the house you've always dreamed of</p>
        <div class="bg-1">
            <div class="container">
                <ul></ul>
                <div class="row text-center">
                    @for (int i = 0; i < (Model).Count; i++)
                    {
                        Html.RenderPartial("PartialViews/HouseLogo", (Model)[i]);
                    }
                </div>
            </div>
            </div>
        </div>
    </body>
</html>
